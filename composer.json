{
    "name": "knops/utilities",
    "description": "Little utilities to make life easier.",
    "type": "library",
    "license": "proprietary",
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Knops\\Utilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Knops\\UtilitiesTests\\": "tests"
        }
    },
    "require-dev": {
        "cocur/slugify": "^4.0",
        "psr/http-message": "^1.0"
    },
    "require": {
        "php": "^8.0"
    }
}
